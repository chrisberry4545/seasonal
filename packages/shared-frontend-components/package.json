{
  "name": "@chrisb-dev/seasonal-shared-frontend-components",
  "version": "1.0.0",
  "homepage": "https://github.com/chrisberry4545/seasonal-shared#readme",
  "description": "Seasonal shared frontend components",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "shared:build": "webpack",
    "prepare": "yarn shared:build",
    "dev": "concurrently \"webpack --watch\" \"yarn test:watch\"",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisberry4545/seasonal-shared.git"
  },
  "author": "Chris Berry <chrisberry4545@googlemail.com> (http://chrisbdev.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chrisberry4545/seasonal-shared/issues"
  },
  "files": [
    "/src"
  ],
  "peerDependencies": {
    "react": "16.9.0"
  },
  "dependencies": {
    "@chrisb-dev/seasonal-shared-models": "1.0.0",
    "@types/node": "^11.11.3",
    "@types/react": "^16.9.0",
    "inuitcss": "^6.0.0",
    "node-sass": "^4.11.0",
    "sass-mq": "^5.0.1",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "jest": "24.9.0",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.2.1",
    "webpack": "4.41.5",
    "webpack-cli": "^3.3.11"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": ".*spec.(ts|tsx)$",
    "setupFilesAfterEnv": [
      "./src/setupTests.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(scss|css)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
