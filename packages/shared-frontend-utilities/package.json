{
  "name": "@chrisb-dev/seasonal-shared-frontend-utilities",
  "version": "1.0.0",
  "description": "Seasonal shared frontend utilities",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "shared:build": "tsc",
    "prepare": "yarn shared:build",
    "start": "concurrently \"yarn dev\" \"yarn test:watch\"",
    "dev": "yarn shared:build && tsc-watch --noClear",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisberry4545/seasonal-shared.git"
  },
  "author": "Chris Berry <chrisberry4545@googlemail.com> (https://chrisberry4545.github.io/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/chrisberry4545/seasonal-shared/issues"
  },
  "files": [
    "/dist"
  ],
  "homepage": "https://github.com/chrisberry4545/seasonal-shared#readme",
  "dependencies": {
    "@chrisb-dev/seasonal-shared-models": "1.0.0",
    "@types/node": "^11.11.3",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "jest": "24.9.0",
    "jest-fetch-mock": "^3.0.3",
    "ts-jest": "^24.0.2",
    "tsc-watch": "^4.0.0"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": ".*spec.ts$",
    "setupFiles": [
      "./src/setupTests.js"
    ]
  }
}
