{
  "name": "seasonal-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index-dev.ts",
  "engines": {
    "node": ">=8.10.0",
    "npm": ">=6.4.1"
  },
  "config": {
    "app": "index-dev.ts"
  },
  "scripts": {
    "start": "node $npm_package_config_app",
    "dev": "concurrently \"npm run copy-env\" \"tsc -w\" \"nodemon dist/index-dev.js\"",
    "build": "tsc && npm run copy-env",
    "copy-env": "cp .env.yml dist/.env.yml",
    "move-dist-to-top-level": "cp -r ./dist/* ./",
    "lint": "semistandard './src/**/*.js' --verbose | snazzy",
    "lint:fix": "semistandard --fix --verbose | snazzy"
  },
  "author": "Chris Berry <chrisberry4545@googlemail.com> (http://chrisbdev.com)",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.16.1",
    "@types/helmet": "0.0.43",
    "airtable": "^0.5.7",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "serverless-http": "^1.9.1",
    "typescript": "^3.3.3333"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "env-yaml": "^0.1.2",
    "nodemon": "^1.18.7",
    "semistandard": "^13.0.1",
    "snazzy": "^8.0.0"
  }
}
